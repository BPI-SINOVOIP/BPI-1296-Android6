	usb2_udc: usb2_udc@981E0000 {
		compatible = "Realtek,rtd129x-usb2-udc";
		reg = <0x981E0000 0x8000>, <0x98013800 0x80>;
		interrupts = <0 21 4>;
		usb-phy = <&ehci_phy_rle0599>;

		status = "disable";
	};

