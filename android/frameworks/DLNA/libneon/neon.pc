prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: neon
Description: HTTP/WebDAV client library
Version: 0.27.2
Libs.private:  -lz -lgssapi_krb5 -lkrb5 -lk5crypto -lcom_err
Libs: -L${libdir} -lneon 
Cflags: -I${includedir}/neon  -D_LARGEFILE64_SOURCE -DNE_LFS
