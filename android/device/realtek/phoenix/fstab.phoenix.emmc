# Android fstab file.
#<src>                                                  <mnt_point>         <type>    <mnt_flags>                                                                         <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/dev/block/mmcblk0p1	/system		ext4	ro,noatime	wait
/dev/block/mmcblk0p2	/data		ext4	rw,noatime,nosuid,nodev,journal_checksum,journal_async_commit,errors=continue,data_err=ignore,discard	wait
/dev/block/mmcblk0p3	/cache		ext4	rw,noatime,nosuid,nodev,journal_checksum,journal_async_commit,errors=continue,data_err=ignore,discard	wait
