<resources>
    <!-- Set the automatic brightness mode on by default -->
    <bool name="def_screen_brightness_automatic_mode">true</bool>

    <!-- Value for Settings.Global.LOW_BATTERY_SOUND_TIMEOUT:
         Mute battery sounds on tablets after one hour with screen off -->
    <integer name="def_low_battery_sound_timeout">3600000</integer>

    <add-resource type="bool" name="def_ethernet_on"/>
        <bool name="def_ethernet_on">true</bool>
    <add-resource type="bool" name="def_ethernet_conf"/>
        <bool name="def_ethernet_conf">true</bool>
    <add-resource type="bool" name="def_ethernet_mode"/>
        <bool name="def_ethernet_mode">true</bool>
    <add-resource type="string" name="def_ethernet_name"/>
        <string name="def_ethernet_name">eth0</string>

    <add-resource type="bool" name="def_pppoe_on"/>
        <bool name="def_pppoe_on">false</bool>
    <add-resource type="string" name="def_pppoe_interface"/>
        <string name="def_pppoe_interface">eth0</string>
    <add-resource type="string" name="def_pppoe_user"/>
        <string name="def_pppoe_user"></string>
    <add-resource type="string" name="def_pppoe_pass"/>
        <string name="def_pppoe_pass"></string>
    <add-resource type="bool" name="def_pppoe_isset"/>
        <bool name="def_pppoe_isset">false</bool>
    <bool name="def_wifi_on">true</bool>

    <bool name="def_wifi_on">true</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_HDMI_OUTPUT_MODE -->
    <!-- 0:RAW, 1:LPCM 2ch, 2:LPCM Multi-ch, 3:Auto -->
    <string name="def_realtek_setup_hdmi_ouput_mode">1</string>

    <!-- Default for Settings.System.REALTEK_DEEP_COLOR_MODE -->
    <!-- AUTO(0), 12BIT(1), 10BIT(2), OFF(3) -->
    <integer name="def_realtek_deep_color_mode">3</integer>

    <integer name="def_screen_off_timeout">300000</integer>
</resources>
